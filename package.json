{
	"name": "@enginemailer/n8n-nodes-enginemailer",
	"version": "1.0.0",
	"description": "n8n node for email marketing using Enginemailer",
	"keywords": [
		"n8n-community-node-package",
		"n8n-nodes",
		"n8n",
		"n8n-nodes-enginemailer",
		"enginemailer",
		"enginemailer-node",
		"enginemailer-nodes"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "Enginemailer Support",
		"email": "support@enginemailer.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Enginemailer-Official/n8n-nodes-enginemailer.git"
	},
	"engines": {
		"node": ">=20.15"
	},
	"main": "index.js",
	"scripts": {
		"build": "npx rimraf dist && tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/EnginemailerV1Api.credentials.js"
		],
		"nodes": [
			"dist/nodes/Enginemailer/Enginemailer.node.js",
			"dist/nodes/Enginemailer/EnginemailerTrigger.node.js"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~8.32.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
